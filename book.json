{
    "title": "System",
    "description": "记录成长点滴",
    "author": "Jason",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "首页": "http://movetobe.github.io"
        }
    },
    "plugins": [
        "back-to-top-button",
        "chapter-fold",
        "code",
