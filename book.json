        "search-pro",
        "github",
        "splitter",
        "tbfed-pagefooter",
        "-sharing",
        "-highlight",
        "styles-less"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/movetobe"
        },
        "code": {
            "copyButtons": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © Jason 2019",
            "modify_label": "本书发布时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
